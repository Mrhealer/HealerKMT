<script type="text/javascript">
    function Init_Prammater() {
        var rootPath        = '';
        var rootPathAPI     = 'http://localhost:8881/api';
        var rootPathUpload  = '';
        var rootPathDepend  = '';
        var rootPathCDN     = '';
        var rootPathReport  = '';
        var rootPathHelp    = '';

        var shareWebName    = '';
        var folderAvatar    = '';
        var folderDoc       = '';
        var folderNew       = '';
        var folderTemp      = '';
        
        var appId           = '';
        var userId          = '';
        var langId          = '';
        var tokenJWT        = '';

        var oConfig = {
            rootPath: rootPath,
            rootPathAPI: rootPathAPI,
            rootPathUpload: rootPathUpload,
            rootPathDepend: rootPathDepend,
            rootPathCDN: rootPathCDN,
            rootPathReport: rootPathReport,
            rootPathHelp: rootPathHelp,

            shareWebName: shareWebName,
            folderAvatar: folderAvatar,
            folderDoc: folderDoc,
            folderNew: folderNew,
            folderTemp: folderTemp,

            appId: appId,
            userId: userId,
            langId: langId,
            tokenJWT:tokenJWT
        };
        
        return oConfig;
    }
    var core = {};
    core['system']   = new systemroot();
    core['extend']   = new systemextend();
    core['constant'] = new constant();
    core['util']     = new util();
    $(document).ready(function () {
        core.system.startApp();
        core.extend.init_sys_extend();
        core.constant.init();
    });
</script>